{
    "name": "vott",
    "version": "3.0.0-alpha.1",
    "private": true,
    "description": "Visual Object Tagging Tool (VoTT) - an annotation and labeling tool for images and video.",
    "homepage": "https://github.com/open-flex/VoTT",
    "repository": {
        "type": "git",
        "url": "https://github.com/open-flex/VoTT.git"
    },
    "license": "MIT",
    "author": {
        "name": "Microsoft",
        "url": "https://github.com/Microsoft/VoTT"
    },
    "main": "build/main.js",
    "scripts": {
        "build": "react-scripts build",
        "compile": "tsc",
        "eject": "react-scripts eject",
        "electron-start": "node src/electron/start",
        "electron:run:dev": "npm run webpack:dev && electron . --remote-debugging-port=9223",
        "electron:run:prod": "npm run webpack:prod && electron . --remote-debugging-port=9223",
        "electron:start:dev": "npm run webpack:dev && npm run electron-start",
        "electron:start:prod": "npm run webpack:prod && npm run electron-start",
        "postinstall": "electron-builder install-app-deps",
        "lint": "eslint 'src/**/*.ts*'",
        "lintfix": "eslint 'src/**/*.ts*' --fix",
        "plato": "scripts/generate-report.sh -o report -v $(node -pe \"require('./package.json').version\") -c $(git rev-parse --short HEAD)",
        "predebug": "npm run build && npm run webpack:dev",
        "prepare": "husky install",
        "react-start": "react-scripts start",
        "release": "npm run build && npm run webpack:prod && electron-builder",
        "release-ci": "bash ./scripts/build.sh",
        "release-pr": "./scripts/release-pr.sh",
        "release-web": "npm run build && npm run webpack:prod",
        "start": "nf start -p 3000",
        "pretest": "eslint 'src/**/*.ts*'",
        "test": "react-scripts test --env=jsdom --silent",
        "test:ci": "cross-env CI=true npm run test",
        "test:coverage": "npm run test -- --coverage",
        "webpack:dev": "webpack --config ./config/webpack.dev.js",
        "webpack:prod": "webpack -p --config ./config/webpack.prod.js"
    },
    "lint-staged": {
        "*.{js,ts,tsx}": "eslint --fix",
        "*.{json,yml,css,scss}": "prettier --write",
        "package.json": "sort-package-json"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "resolutions": {
        "react-jsonschema-form": "1.3.0",
        "webpack": "4.44.2"
    },
    "dependencies": {
        "@azure/storage-blob": "^10.3.0",
        "@fortawesome/fontawesome-free": "^5.5.0",
        "@microsoft/applicationinsights-web": "^2.7.2",
        "@tensorflow/tfjs": "^1.0.3",
        "@tensorflow/tfjs-core": "^1.0.3",
        "axios": "^0.21.1",
        "bootstrap": "^4.1.3",
        "buffer-reverse": "^1.0.1",
        "crypto-js": "^3.1.9-1",
        "dotenv": "^7.0.0",
        "express-request-id": "^1.4.1",
        "google-protobuf": "^3.6.1",
        "history": "^4.7.2",
        "jquery": "^3.3.1",
        "json2csv": "^4.5.0",
        "lodash": "^4.17.11",
        "md5.js": "^1.3.5",
        "node-int64": "^0.4.0",
        "popper.js": "^1.14.6",
        "rc-align": "^2.4.5",
        "rc-checkbox": "^2.3.2",
        "rc-menu": "^7.4.29",
        "rc-slider": "^9.7.5",
        "react": "^16.7.0",
        "react-appinsights": "^3.0.0-rc.5",
        "react-color": "^2.17.0",
        "react-dnd": "^14.0.2",
        "react-dnd-html5-backend": "^14.0.0",
        "react-dom": "^16.7.0",
        "react-jsonschema-form": "^1.3.0",
        "react-localization": "^1.0.13",
        "react-redux": "^5.1.1",
        "react-router-dom": "^4.3.1",
        "react-split-pane": "^0.1.87",
        "react-toastify": "^4.5.2",
        "react-virtualized": "^9.21.0",
        "react-vis": "^1.11.6",
        "reactstrap": "^6.5.0",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "rimraf": "^2.6.2",
        "shortid": "^2.2.14",
        "video-react": "^0.13.2",
        "vott-ct": "2.1.24",
        "vott-react": "^0.2.10"
    },
    "devDependencies": {
        "@types/dotenv": "^6.1.0",
        "@types/enzyme": "^3.1.15",
        "@types/jest": "23.3.9",
        "@types/json2csv": "^4.4.0",
        "@types/lodash": "^4.14.178",
        "@types/node": "14.6.2",
        "@types/react": "16.7.6",
        "@types/react-dom": "16.0.9",
        "@types/react-jsonschema-form": "^1.0.12",
        "@types/react-router-dom": "^4.3.1",
        "@types/react-split-pane": "^0.1.67",
        "@types/react-toastify": "^4.0.1",
        "@types/reactstrap": "^6.4.3",
        "@types/redux-logger": "^3.0.6",
        "@types/redux-mock-store": "^1.0.0",
        "@types/snapsvg": "^0.4.35",
        "cross-env": "^5.2.0",
        "electron": "^13.6.3",
        "electron-builder": "^22.6.0",
        "enzyme": "^3.7.0",
        "enzyme-adapter-react-16": "^1.7.0",
        "foreman": "^3.0.1",
        "husky": "^7.0.4",
        "jest-enzyme": "^7.0.1",
        "lint-staged": "^12.2.0",
        "mock-fs": "^4.13.0",
        "node-sass": "^4.14.1",
        "prettier": "^2.5.1",
        "react-scripts": "4.0.3",
        "redux-immutable-state-invariant": "^2.1.0",
        "redux-logger": "^3.0.6",
        "redux-mock-store": "^1.5.3",
        "sort-package-json": "^1.53.1",
        "ts-loader": "^5.3.0",
        "typescript": "^3.8.0",
        "webpack": "^4.19.1",
        "webpack-cli": "^3.1.2",
        "webpack-merge": "^4.1.5"
    },
    "engines": {
        "node": ">=14.18.2",
        "npm": ">=7.0.0"
    },
    "productName": "VoTT"
}
